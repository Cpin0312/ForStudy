[core]
	repositoryformatversion = 0
	filemode = false
	bare = false
	logallrefupdates = true
	symlinks = false
	ignorecase = true
[remote "origin"]
	url = https://github.com/Cpin0312/ForStudy.git
	fetch = +refs/heads/*:refs/remotes/origin/*
[branch "master"]
	remote = origin
	merge = refs/heads/master
[branch "myBranch"]
	remote = origin
	merge = refs/heads/master
[diff]
    tool = WinMerge
[difftool "WinMerge"]
    path = D:/0_MyFolder/03_Installer/WinMerge/WinMergeU.exe
    cmd = \"D:/0_MyFolder/03_Installer/WinMerge/WinMergeU.exe\" -f \"*.*\" -e -u -r \"$LOCAL\" \"$REMOTE\"
[merge]
    tool = WinMerge
[mergetool "WinMerge"]
    path = D:/0_MyFolder/03_Installer/WinMerge/WinMergeU.exe
    cmd = \"D:/0_MyFolder/03_Installer/WinMerge/WinMergeU.exe\" -e -u \"$LOCAL\" \"$REMOTE\" \"$MERGED\"
[alias]
    windiff = difftool -y -d -t WinMerge
    winmerge = mergetool -y -t WinMerge
    sts = status
    showlog = log --pretty=format:"[%H][%cd][%cn]:[%s]"
    pushF = push origin HEAD:master
    cmt = commit -m 





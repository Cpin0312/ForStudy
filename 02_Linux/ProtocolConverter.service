[Unit]
Description = Java Service
After = network.target

[Service]
Type = forking
Restart=always
RestartSec=1
SuccessExitStatus=143 
ExecStart=@APP_DIR@/ProtocolConverter.sh start
ExecStop=@APP_DIR@/ProtocolConverter.sh stop
ExecReload=@APP_DIR@/ProtocolConverter.sh reload

[Install]
WantedBy=multi-user.target
